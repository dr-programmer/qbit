I (
    1, 0, 
    0, 1
)

X (
    0, 1, 
    1, 0
)

H (
    1 / #2, 1 / #2, 
    1 / #2, -(1 / #2)
)

CNOT (
        |0>*<0| @ (I) + |1>*<1| @ (X)
)

